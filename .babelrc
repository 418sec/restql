{
  "env": {
    "development": {
      "plugins": [
        "transform-object-rest-spread",
        "transform-runtime"
      ],
      "presets": [
        "env"
      ]
    },
    "production": {
      "plugins": [
        "external-helpers",
        "transform-object-rest-spread",
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false
        }]
      ],
      "presets": [
        ["env", {
          "modules": false
        }]
      ]
    },
    "test": {
      "plugins": [
        "transform-object-rest-spread",
        "transform-runtime"
      ],
      "presets": [
        "env"
      ]
    }
  }
}
